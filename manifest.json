{
    "manifest_version": 3,
    "name": "Clickable Phone Numbers for Google Sheets",
    "version": "1.0",
    "description": "Make phone numbers clickable in Google Sheets",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
  